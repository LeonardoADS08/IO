<Window x:Class="IO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IO"
        mc:Ignorable="d"
        Title="IO Solver" Height="720" Width="1280" Background="#ecf0f1">
    <Grid>
        <Grid Height="120" VerticalAlignment="Top" Background="#FF27AE60">
            <Button x:Name="B_Inicio"  Width="220" Height="40" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="Inicio" FontSize="22" Background="#FF39BF72" BorderBrush="{x:Null}" Foreground="#FF34495E" Click="B_Inicio_Click" ></Button>
            <Button x:Name="B_Grafico"  Width="220" Height="40" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="Grafico" FontSize="22" Background="#FF39BF72" BorderBrush="{x:Null}" Foreground="#FF34495E" Margin="219,0,0,0" ></Button>
            <Button x:Name="B_Simplex"  Width="220" Height="40" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="Simplex" FontSize="22" Background="#FF39BF72" BorderBrush="{x:Null}" Foreground="#FF34495E" Margin="438,0,0,0" ></Button>
        </Grid>
        <Frame x:Name="F_Imagen" Margin="0,120,0,0"></Frame>
    </Grid>
</Window>
